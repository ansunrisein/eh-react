    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "apollo": "^2.31.1",
    "classnames": "^2.2.6",
    "graphql": "^15.4.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-app-rewire-aliases": "^0.2.0",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "react-test-renderer": "^17.0.1",
    "react-timer-hook": "^2.0.3",
    "reflexbox": "^4.0.6",
    "rsuite": "^4.8.5",
    "sb": "6.1.8",
    "swiper": "^6.3.5",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "codegen": "apollo client:codegen types --no-addTypename --target=typescript --globalTypesFile=./src/.types/globalTypes.ts --localSchemaFile=./schema.graphql",
    "lint": "eslint src/",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(js|jsx|ts|tsx)",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.1.9",
    "@storybook/addon-essentials": "6.1.9",
    "@storybook/addon-links": "6.1.9",
    "@storybook/addons": "^6.1.10",
    "@storybook/node-logger": "6.1.9",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "6.1.9",
    "@storybook/theming": "^6.1.10",
    "@types/classnames": "^2.2.11",
    "@types/reflexbox": "^4.0.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "jest": {
    "moduleNameMapper": {
      "@eh/react/(.*)$": "<rootDir>/src/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty-quick && yarn lint --max-warnings=0"
    }
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  }
}
